{
	"package": {
		"name": "origin",
		"repo": "helloworld",
		"subject": "alexeyya",
		"vcs_url": "https://github.com/AlexeyYa/origin.git",
		"licenses": ["MIT"]
	},
	
	"version": {
		"name": "0.1.${TRAVIS_BUILD_NUMBER}"
		"released": "${build.timestamp}"
	},
	
	"files": [{
		"includePattern": "/home/travis/build/AlexeyYa/origin/build/(.*\.deb)", 
		"upladPattern": "pool/main/m/mypackage/$1",
		"matrixParams": {
			"deb_distribution": "vivid",
			"deb_component": "main",
			"deb_architecture": "amd64"
		}
	}],
	
	"publish": true
}